{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2.5",
        "aws/aws-sdk-php": "^3.183",
        "barryvdh/laravel-dompdf": "^0.9.0",
        "bashy/laravel-campaignmonitor": "^5.0",
        "brian2694/laravel-toastr": "^5.54",
        "cartalyst/stripe": "~2.0",
        "clxcommunications/sdk-xms": "^1.1",
        "fideloper/proxy": "^4.2",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^6.5|^7.0.1",
        "kreait/laravel-firebase": "^3.1",
        "laravel-notification-channels/fcm": "^2.2",
        "laravel/framework": "^7.30|^8.0",
        "laravel/passport": "^9.3",
        "laravel/socialite": "^5.2",
        "laravel/tinker": "^2.0",
        "laravel/ui": "^2.5.0|^3.0",
        "laravelcollective/html": "^6.2",
        "maatwebsite/excel": "^3.1",
        "proengsoft/laravel-jsvalidation": "^4.1",
        "propaganistas/laravel-phone": "^4.2",
        "stripe/stripe-php": "^7.61"
    },
    "require-dev": {
        "facade/ignition": "^2.3.6",
        "fzaninotto/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "nunomaduro/collision": "^4.3.0|^5.0",
        "phpunit/phpunit": "^8.5.14|^9.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "app/Libraries"
        ],
        "files": [
            "app/helpers/custom.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
